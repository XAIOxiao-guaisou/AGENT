{
    "DEEPSEEK_API_KEY": "",
    "DEEPSEEK_API_URL": "https://api.deepseek.com/v1/chat/completions",
    "MODEL_NAME": "deepseek-reasoner",
    "TEMPERATURE": 0.0,
    "MAX_TOKENS": 4096,
    "RETRY_LIMIT": 3,
    "ANTI_BOT_DELAY_RANGE": [
        2.0,
        5.0
    ],
    "PROTECTED_PATHS": [
        "antigravity",
        "config",
        "tests/antigravity",
        "tasks.json",
        "requirements.txt"
    ],
    "IGNORE_PATTERNS": [
        "**/__pycache__/**",
        "**/.pytest_cache/**",
        "**/.git/**",
        "**/node_modules/**",
        "**/venv/**",
        "**/env/**",
        "**/build/**",
        "**/dist/**",
        "**/*.pyc",
        "**/*.pyo",
        "**/*.log",
        "**/data/**",
        "**/.vscode/**",
        "**/.idea/**",
        "**/state.json",
        "**/VIBE_FIX.md"
    ],
    "WATCH_EXTENSIONS": [
        ".py",
        ".js",
        ".tsx",
        ".ts",
        ".md"
    ]
}